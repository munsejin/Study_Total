package festival;

import java.util.Scanner;

public class no4 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("정수 입력 >> ");
		int number = sc.nextInt();
		int sum = 0;
		for (int i = 1; i<number; i++) {
			sum += i;
			System.out.print(sum + " ");
	}
	}
}

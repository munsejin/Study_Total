package festival;

public class no10 {

	public static void main(String[] args) {
		int arr[][] = new int[7][7];

		int start = 4;
		int end = 4;

		System.out.println(" ¿øº» >> ");
		int cnt = 1;
		for (int i = 0; i < end; i++) {
			for (int j = start; j < arr.length; j++) {
				arr[i][j] = cnt++;
				System.out.print(arr[i][j] + "\t");
			}
			System.out.println(" ");
		}
	}

}

package festival;

public class no10 {

	public static void main(String[] args) {
		int arr[][] = new int[5][5];
		
		int start = 2;
		int end = 2;

		System.out.println(" ¿øº» >> ");
		int cnt = 1;
		for (int i = start; i <=end; i++) {
			for (int j = 0; j <= start; j++) {
				arr[i][j] = cnt++;
				System.out.print(arr[i][j] + "\t");
			}
			System.out.println(" ");
		}
	}

}

package festival;

public class no8 {

	public static void main(String[] args) {

		int arr[][] = new int[5][5];

		System.out.println(" ¿øº» >> ");
		int cnt = 1;
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr.length; j++) {
				arr[i][j] = cnt++;
				System.out.print(arr[i][j] + "\t");
			}
			System.out.println(" ");
		}

		System.out.println(" 180µµ ");

		int start = 4;
		int end = 0;

		for (int i = arr.length - 1; i < 0; i--) {
			for (int j = arr.length - 1; j < end; j--) {
				arr[i][j] = cnt++;
				System.out.print(arr[i][j] + "\t");
			}
			System.out.println(" ");
		}

	}

}
